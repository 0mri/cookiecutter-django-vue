{
  "name": "{{cookiecutter.project_slug}}",
  "description": "{{cookiecutter.description}}",
  "version": "{{cookiecutter.version}}",
  "author": "{{cookiecutter.author}}",
  {% if cookiecutter.license != 'Not open source' %}"license": "{{cookiecutter.license}}",{% endif %}
  {% if cookiecutter.license == 'Not open source' %}"private": true,{% endif %}
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint --ext .js,.vue {{cookiecutter.project_slug}}/static"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.4",
    "foundation-sites": "^6.4.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    {% if cookiecutter.static_and_media == 'Amazon S3 (static and media)' %}"string-replace-loader": "^1.3.0",{% endif %}
    "url-loader": "^0.5.8",
    "vue": "^2.3.4",
    {% if cookiecutter.analytics == "Google Analytics" %}"vue-analytics": "^5.3.2",{% endif %}
    "vue-loader": "^13.3.0",
    "vue-meta": "^1.2.0",
    {% if cookiecutter.use_progressbar == 'y' %}"vue-progressbar": "^0.7.3",{% endif %}
    {% if cookiecutter.use_sentry == 'y' %}"vue-raven": "^0.1.0",{% endif %}
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.3.4",
    {% if cookiecutter.analytics == "Yandex Metrika" %}"vue-yandex-metrika": "^1.4.0",{% endif %}
    "vuex": "^2.3.1",
    "webpack": "^2.6.1",
    "webpack-bundle-tracker": "0.2.1",
    {% if cookiecutter.static_and_media == 'Amazon S3 (static and media)' %}"webpack-s3-plugin": "git+https://github.com/eyupci/s3-plugin-webpack.git#master",{% endif %}
    "webpack-dev-server": "^2.4.5"
  }
}
